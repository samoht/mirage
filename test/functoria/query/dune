 (name config)
 (libraries f0))

(include dune.inc)

(rule
 (deps
  (source_tree .))
 (action
  (with-stdout-to
   dune.gen
   (run ocaml gen.ml))))

(rule
 (alias runtest)
 (package functoria)
 (action
  (diff dune.inc dune.gen)))

